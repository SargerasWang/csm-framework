<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="sys_cache">
    <cache/>
    <select id="selectUserNameById" resultType="String">
        SELECT name from sys_user_account where id = #{id}
    </select>
    <select id="selectRoleNameById" resultType="String">
        SELECT name from sys_role where id = #{id}
    </select>

</mapper>